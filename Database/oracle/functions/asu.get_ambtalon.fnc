DROP FUNCTION ASU.GET_AMBTALON
/

--
-- GET_AMBTALON  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TAMBTALON (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_AMBTALON" (pFK_AMB IN NUMBER) RETURN  NUMBER IS
 nRes NUMBER;
BEGIN
    SELECT NVL(MAX(FK_ID), 0) INTO nRES FROM ASU.TAMBTALON WHERE FK_AMBID = pFK_AMB;
    RETURN nRES ;
END;
/

SHOW ERRORS;


