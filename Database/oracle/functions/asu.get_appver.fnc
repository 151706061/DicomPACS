DROP FUNCTION ASU.GET_APPVER
/

--
-- GET_APPVER  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TAPPVIEW (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_APPVER" 
  ( pFK_ID IN NUMBER)
  RETURN  VARCHAR2 IS
  str VARCHAR2(10);
  CURSOR c IS SELECT FC_VER FROM TAPPVIEW WHERE FK_ID=pFK_ID;
BEGIN
  if c%ISOPEN THEN
    CLOSE c;
  END IF;
  OPEN C;
  FETCH C INTO str;
  CLOSE C;
  RETURN str;
END;
/

SHOW ERRORS;


