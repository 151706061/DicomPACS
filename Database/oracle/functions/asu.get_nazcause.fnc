DROP FUNCTION ASU.GET_NAZCAUSE
/

--
-- GET_NAZCAUSE  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TSMID (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_NAZCAUSE" RETURN NUMBER IS
   CURSOR cID IS SELECT FK_ID FROM TSMID WHERE FC_SYNONIM = 'NAZCAUSE';
   pFK_ID NUMBER;
BEGIN
 OPEN cID;
 FETCH cID INTO pFK_ID;
 CLOSE cID;
 RETURN pFK_ID;
END;
/

SHOW ERRORS;


