DROP FUNCTION ASU.GET_NAZDATE
/

--
-- GET_NAZDATE  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TNAZAN (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_NAZDATE" (pFK_ID IN NUMBER) RETURN  DATE IS
    CURSOR cDATE IS SELECT FD_NAZ FROM TNAZAN WHERE FK_ID = pFK_ID;
    dFD_DATE DATE;
BEGIN
    OPEN cDATE;
    FETCH cDATE INTO dFD_DATE;
    CLOSE cDATE;
    RETURN dFD_DATE;
END;
/

SHOW ERRORS;


