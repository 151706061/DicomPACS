DROP FUNCTION ASU.GET_NAZMARK
/

--
-- GET_NAZMARK  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TNAZMARK (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_NAZMARK" (ANAZID IN TNAZMARK.FK_NAZID%TYPE) RETURN INTEGER IS
  RESULT INTEGER;
BEGIN
  SELECT FL_NAZMARK INTO RESULT
  FROM TNAZMARK
  WHERE FK_NAZID = ANAZID
  AND ROWNUM < 2;
  RETURN(RESULT);
EXCEPTION WHEN OTHERS THEN RETURN(0);
END GET_NAZMARK;
/

SHOW ERRORS;


