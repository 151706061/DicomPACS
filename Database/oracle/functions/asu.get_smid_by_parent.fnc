DROP FUNCTION ASU.GET_SMID_BY_PARENT
/

--
-- GET_SMID_BY_PARENT  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   VNAZ (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_SMID_BY_PARENT" (pFK_ID IN NUMBER)
RETURN NUMBER IS
SMIDID NUMBER;
CURSOR c IS SELECT FK_SMID FROM VNAZ WHERE FK_ID = pFK_ID;
BEGIN
  OPEN c;
  FETCH c INTO SMIDID;
  CLOSE c;

  RETURN SMIDID;
END;
/

SHOW ERRORS;


