DROP FUNCTION ASU.GET_SMID_TYPE
/

--
-- GET_SMID_TYPE  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TSMID (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_SMID_TYPE" ( pFK_ID IN NUMBER ) RETURN VARCHAR2 IS
    CURSOR cTYPE IS SELECT FC_TYPE FROM TSMID WHERE FK_ID = pFK_ID;
    strTYPE VARCHAR2(25);
BEGIN
    OPEN cTYPE;
    FETCH cTYPE INTO strTYPE;
    CLOSE cTYPE;
    RETURN strTYPE;
END;
/

SHOW ERRORS;


