DROP FUNCTION ASU.GET_SMIDFROMGROUP
/

--
-- GET_SMIDFROMGROUP  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TNAZGROUP (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_SMIDFROMGROUP" (pFK_GROUP IN NUMBER) RETURN  NUMBER IS
    CURSOR c IS SELECT FK_SMID FROM TNAZGROUP WHERE FK_GROUPID = pFK_GROUP AND ROWNUM = 1;
    nRes NUMBER;
BEGIN
    OPEN c;
    FETCH c INTO nRes;
    Close c;
    RETURN nRes;
END;
/

SHOW ERRORS;


