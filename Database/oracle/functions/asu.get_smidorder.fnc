DROP FUNCTION ASU.GET_SMIDORDER
/

--
-- GET_SMIDORDER  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TSMID (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_SMIDORDER" (pFK_ID IN NUMBER)
RETURN NUMBER IS
CURSOR cORDER IS SELECT FN_ORDER FROM TSMID WHERE FK_ID = pFK_ID;
ORD NUMBER;
BEGIN
OPEN cORDER;
FETCH cORDER INTO ORD;
CLOSE cORDER;
 RETURN ORD;
END;
/

SHOW ERRORS;


