DROP FUNCTION ASU.GET_SOSBYNAZ
/

--
-- GET_SOSBYNAZ  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TNAZAN (Table)
--
CREATE OR REPLACE FUNCTION ASU."GET_SOSBYNAZ" (pFK_ID IN NUMBER) RETURN NUMBER IS
    CURSOR cSOS IS SELECT FK_NAZSOSID FROM TNAZAN WHERE FK_ID = pFK_ID;
    nFK_SOS NUMBER;
BEGIN
    OPEN cSOS;
    FETCH cSOS INTO nFK_SOS;
    CLOSE cSOS;
    RETURN nFK_SOS;
END;
/

SHOW ERRORS;


