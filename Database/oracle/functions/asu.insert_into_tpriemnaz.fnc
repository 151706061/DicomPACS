/

--
-- INSERT_INTO_TPRIEMNAZ  (Function) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TPRIEMNAZ (Table)
--
CREATE OR REPLACE FUNCTION ASU."INSERT_INTO_TPRIEMNAZ" (pFK_PARENTID IN NUMBER, pFK_NAZID IN NUMBER)
RETURN NUMBER IS
pFK_ID NUMBER;
BEGIN
INSERT INTO TPRIEMNAZ(FK_PARENTID, FK_NAZID)
VALUES(pFK_PARENTID, pFK_NAZID)
RETURNING FK_ID INTO pFK_ID;

  RETURN pFK_ID ;
END;
/

