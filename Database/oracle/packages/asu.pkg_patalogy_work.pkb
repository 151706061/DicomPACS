DROP PACKAGE BODY ASU.PKG_PATALOGY_WORK
/

--
-- PKG_PATALOGY_WORK  (Package Body) 
--
CREATE OR REPLACE PACKAGE BODY ASU."PKG_PATALOGY_WORK" IS
  PROCEDURE SET_SHOWN (pFK_ID IN NUMBER) IS
    PRAGMA AUTONOMOUS_TRANSACTION;
  BEGIN
    UPDATE TPATALOGY
       SET FL_SHOWN = 1
     WHERE FK_VRACHID = pFK_ID
       AND FL_SHOWN = 0;
    COMMIT;
  END;
END;
-- Package Body PKG_PATALOGY_WORK
/

SHOW ERRORS;


