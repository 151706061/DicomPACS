DROP PROCEDURE ASU.DO_CHANGE_STREET
/

--
-- DO_CHANGE_STREET  (Procedure) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TSTREET (Table)
--
CREATE OR REPLACE PROCEDURE ASU."DO_CHANGE_STREET" 
   ( pFK_ID IN NUMBER,pFC_NAME IN VARCHAR2)
   IS
BEGIN
  UPDATE TSTREET SET FC_NAME=pFC_NAME WHERE FK_ID=pFK_ID;
END; -- Procedure
/

SHOW ERRORS;


