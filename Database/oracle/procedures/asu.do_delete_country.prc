DROP PROCEDURE ASU.DO_DELETE_COUNTRY
/

--
-- DO_DELETE_COUNTRY  (Procedure) 
--
--  Dependencies: 
--   STANDARD (Package)
--   SYS_STUB_FOR_PURITY_ANALYSIS (Package)
--   TCOUNTRY (Table)
--
CREATE OR REPLACE PROCEDURE ASU."DO_DELETE_COUNTRY" 
   ( pFK_ID IN NUMBER)
   IS
BEGIN
  DELETE FROM TCOUNTRY WHERE FK_ID=pFK_ID;
END; -- Procedure
/

SHOW ERRORS;


