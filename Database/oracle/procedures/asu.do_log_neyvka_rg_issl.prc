DROP PROCEDURE ASU.DO_LOG_NEYVKA_RG_ISSL
/

--
-- DO_LOG_NEYVKA_RG_ISSL  (Procedure) 
--
--  Dependencies: 
--   STANDARD (Package)
--   TLOG_NEYVKA (Table)
--
CREATE OR REPLACE PROCEDURE ASU."DO_LOG_NEYVKA_RG_ISSL" (s VARCHAR2)  
IS PRAGMA AUTONOMOUS_TRANSACTION;

BEGIN
  INSERT INTO TLOG_NEYVKA(FC_ACTION, FD_DATE)
  VALUES(s, SYSDATE);
  COMMIT;
END; -- Procedure
/

