DROP TRIGGER ASU.PASPORT_MIC_BEFORE_INSERT
/

--
-- PASPORT_MIC_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_PASPORT_MIC (Sequence)
--   PASPORT_MIC (Table)
--
CREATE OR REPLACE TRIGGER ASU."PASPORT_MIC_BEFORE_INSERT" 
  BEFORE INSERT
  ON ASU.pasport_mic
  REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
BEGIN
  SELECT ASU.SEQ_pasport_mic.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


