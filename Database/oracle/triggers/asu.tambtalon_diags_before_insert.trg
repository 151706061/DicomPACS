DROP TRIGGER ASU.TAMBTALON_DIAGS_BEFORE_INSERT
/

--
-- TAMBTALON_DIAGS_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   TAMBTALON_DIAGS (Table)
--   SEQ_TAMBTALON_DIAGS (Sequence)
--
CREATE OR REPLACE TRIGGER ASU."TAMBTALON_DIAGS_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON tambtalon_diags
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT SEQ_TAMBTALON_DIAGS.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


