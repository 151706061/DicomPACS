DROP TRIGGER ASU.TANALPROC_BEFORE_INSERT
/

--
-- TANALPROC_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   TANALPROC (Table)
--   SEQ_ANALPROC (Sequence)
--
CREATE OR REPLACE TRIGGER ASU."TANALPROC_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON tanalproc
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT seq_analproc.NEXTVAL INTO :NEW.fk_id FROM DUAL;
END;
/
SHOW ERRORS;


