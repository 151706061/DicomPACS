DROP TRIGGER ASU.TAPPREQ_INSERT
/

--
-- TAPPREQ_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   TAPPREQ (Table)
--   SEQ_TAPP (Sequence)
--
CREATE OR REPLACE TRIGGER ASU."TAPPREQ_INSERT" 
  BEFORE INSERT
  ON ASU.TAPPREQ   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
Begin
  SELECT SEQ_TAPP.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


