DROP TRIGGER ASU.TAPRIHVID$INS$UPD_APPID
/

--
-- TAPRIHVID$INS$UPD_APPID  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   TAPRIHVID (Table)
--
CREATE OR REPLACE TRIGGER ASU."TAPRIHVID$INS$UPD_APPID" 
BEFORE  INSERT  OR UPDATE  OF
   FK_APPID
 ON ASU.TAPRIHVID REFERENCING
 NEW AS NEW
 OLD AS OLD
FOR EACH ROW
BEGIN
  IF :new.fk_appid = -1 THEN
    :new.fk_appid := NULL;
  END IF;
END;
/
SHOW ERRORS;


