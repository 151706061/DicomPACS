DROP TRIGGER ASU.TBILL_USLUG_BEFORE_INSERT
/

--
-- TBILL_USLUG_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TBILL_USLUG (Sequence)
--   TBILL_USLUG (Table)
--
CREATE OR REPLACE TRIGGER ASU."TBILL_USLUG_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON tbill_uslug
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT SEQ_TBILL_USLUG.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


