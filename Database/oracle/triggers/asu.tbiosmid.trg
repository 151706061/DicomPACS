DROP TRIGGER ASU.TBIOSMID
/

--
-- TBIOSMID  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TBIOSMID (Sequence)
--
CREATE OR REPLACE TRIGGER ASU."TBIOSMID" 
BEFORE  INSERT  ON ASU.TBIOSMID REFERENCING
 NEW AS NEW
 OLD AS OLD
FOR EACH ROW
Begin
  SELECT SEQ_TBIOSMID.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


