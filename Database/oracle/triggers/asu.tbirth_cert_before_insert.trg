DROP TRIGGER ASU.TBIRTH_CERT_BEFORE_INSERT
/

--
-- TBIRTH_CERT_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TBIRTH_CERTIFICATION (Sequence)
--   TBIRTH_CERTIFICATION (Table)
--
CREATE OR REPLACE TRIGGER ASU."TBIRTH_CERT_BEFORE_INSERT" 
  BEFORE INSERT
  ON ASU.TBIRTH_CERTIFICATION   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
Begin
  SELECT SEQ_TBIRTH_CERTIFICATION.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


