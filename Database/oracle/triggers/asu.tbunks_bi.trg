DROP TRIGGER ASU.TBUNKS_BI
/

--
-- TBUNKS_BI  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TBUNKS (Sequence)
--   TBUNKS (Table)
--
CREATE OR REPLACE TRIGGER ASU."TBUNKS_BI" 
 BEFORE
  INSERT
 ON ASU.TBUNKS REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
Begin
  SELECT ASU.SEQ_TBUNKS.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


