DROP TRIGGER ASU.TCHEMOTHERAPY_TEMPL_BI
/

--
-- TCHEMOTHERAPY_TEMPL_BI  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TCHEMOTHERAPY_TEMPL (Sequence)
--   TCHEMOTHERAPY_TEMPL (Table)
--
CREATE OR REPLACE TRIGGER ASU."TCHEMOTHERAPY_TEMPL_BI" 
 BEFORE
 INSERT
 ON ASU.TCHEMOTHERAPY_TEMPL  REFERENCING OLD AS OLD NEW AS NEW
 FOR EACH ROW
BEGIN
   select SEQ_TCHEMOTHERAPY_TEMPL.nextval into :new.fk_id from dual;
END;
/
SHOW ERRORS;


