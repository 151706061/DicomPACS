DROP TRIGGER ASU.TDIAGTEMPLATE_BEFORE_INSERT
/

--
-- TDIAGTEMPLATE_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TDIAGTEMPLATE (Sequence)
--   TDIAGTEMPLATE (Table)
--
CREATE OR REPLACE TRIGGER ASU."TDIAGTEMPLATE_BEFORE_INSERT" 
BEFORE  INSERT  ON ASU.TDIAGTEMPLATE FOR EACH ROW
Begin
  SELECT SEQ_TDIAGTEMPLATE.NEXTVAL INTO :NEW.FK_ID FROM DUAl;
End;
/
SHOW ERRORS;


