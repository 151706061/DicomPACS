DROP TRIGGER ASU.TDIALIZ_KARTA_BEFORE_INSERT
/

--
-- TDIALIZ_KARTA_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TKARTA (Sequence)
--   TDIALIZ_KARTA (Table)
--
CREATE OR REPLACE TRIGGER ASU."TDIALIZ_KARTA_BEFORE_INSERT"
  BEFORE INSERT
  ON ASU.TDIALIZ_KARTA   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
BEGIN
  SELECT ASU.SEQ_TKARTA.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


