DROP TRIGGER ASU.TDIALIZ_TECH_SHBN_BEFORE_INS
/

--
-- TDIALIZ_TECH_SHBN_BEFORE_INS  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TDIALIZ_TECH_SHABLON (Sequence)
--   TDIALIZ_TECH_SHABLON (Table)
--
CREATE OR REPLACE TRIGGER ASU."TDIALIZ_TECH_SHBN_BEFORE_INS"
  BEFORE INSERT
  ON ASU.TDIALIZ_TECH_SHABLON   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
BEGIN
  SELECT ASU.SEQ_TDIALIZ_TECH_SHABLON.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


