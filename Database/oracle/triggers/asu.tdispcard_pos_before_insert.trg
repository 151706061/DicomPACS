DROP TRIGGER ASU.TDISPCARD_POS_BEFORE_INSERT
/

--
-- TDISPCARD_POS_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TDISPCARD_POS (Sequence)
--   TDISPCARD_POS (Table)
--
CREATE OR REPLACE TRIGGER ASU."TDISPCARD_POS_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON tdispcard_pos
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
Begin
  SELECT SEQ_TDispCard_Pos.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


