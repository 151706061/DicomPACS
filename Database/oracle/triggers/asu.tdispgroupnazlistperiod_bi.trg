DROP TRIGGER ASU.TDISPGROUPNAZLISTPERIOD_BI
/

--
-- TDISPGROUPNAZLISTPERIOD_BI  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TDISPGROUPNAZLISTPERIOD (Sequence)
--   TDISPGROUPNAZLISTPERIOD (Table)
--
CREATE OR REPLACE TRIGGER ASU."TDISPGROUPNAZLISTPERIOD_BI" 
BEFORE INSERT
ON ASU.TDISPGROUPNAZLISTPERIOD REFERENCING OLD AS OLD NEW AS NEW
FOR EACH ROW
Begin
  SELECT SEQ_TDISPGROUPNAZLISTPERIOD.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


