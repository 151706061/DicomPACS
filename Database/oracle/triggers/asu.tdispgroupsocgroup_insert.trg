DROP TRIGGER ASU.TDISPGROUPSOCGROUP_INSERT
/

--
-- TDISPGROUPSOCGROUP_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TDISPGROUPSOCGROUP (Sequence)
--   TDISPGROUPSOCGROUP (Table)
--
CREATE OR REPLACE TRIGGER ASU."TDISPGROUPSOCGROUP_INSERT" 
  BEFORE INSERT
  ON ASU.TDISPGROUPSOCGROUP   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
Begin
  select SEQ_TDISPGROUPSOCGROUP.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


