DROP TRIGGER ASU.TDISPGROUPSTADIA_INSERT
/

--
-- TDISPGROUPSTADIA_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TDISPGROUPSTADIA (Sequence)
--   TDISPGROUPSTADIA (Table)
--
CREATE OR REPLACE TRIGGER ASU."TDISPGROUPSTADIA_INSERT" 
  BEFORE INSERT
  ON ASU.TDISPGROUPSTADIA   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
Begin
  select SEQ_TDISPGROUPSTADIA.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


