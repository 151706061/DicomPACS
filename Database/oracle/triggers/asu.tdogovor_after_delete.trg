DROP TRIGGER ASU.TDOGOVOR_AFTER_DELETE
/

--
-- TDOGOVOR_AFTER_DELETE  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   TDOGOVOR (Table)
--   TKARTA (Table)
--
CREATE OR REPLACE TRIGGER ASU."TDOGOVOR_AFTER_DELETE" 
AFTER DELETE
ON ASU.TDOGOVOR REFERENCING OLD AS OLD NEW AS NEW
FOR EACH ROW
Begin
  DELETE FROM TKARTA WHERE FK_KOD=:OLD.FK_ID AND FK_KOD2=2 AND FP_TEK_COC=1;
End;
/
SHOW ERRORS;


