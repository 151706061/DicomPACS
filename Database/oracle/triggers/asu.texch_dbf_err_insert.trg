DROP TRIGGER ASU.TEXCH_DBF_ERR_INSERT
/

--
-- TEXCH_DBF_ERR_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TEXCH_DBF_ERR (Sequence)
--   TEXCH_DBF_ERR (Table)
--
CREATE OR REPLACE TRIGGER ASU."TEXCH_DBF_ERR_INSERT" 
 BEFORE
  INSERT
 ON asu.TEXCH_DBF_ERR
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
Begin
  IF :new.fk_id IS NULL THEN
    SELECT asu.SEQ_TEXCH_DBF_ERR.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
  end if;
End;
/
SHOW ERRORS;


