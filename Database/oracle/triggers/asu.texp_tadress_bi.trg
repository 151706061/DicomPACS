DROP TRIGGER ASU.TEXP_TADRESS_BI
/

--
-- TEXP_TADRESS_BI  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TEXP_TADR (Sequence)
--   TEXP_TADRESS (Table)
--
CREATE OR REPLACE TRIGGER ASU.TEXP_TADRESS_BI
 BEFORE INSERT
 ON ASU.TEXP_TADRESS REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
 IF :NEW.FK_ID IS NULL THEN
    SELECT ASU.SEQ_TEXP_TADR.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
 END IF;
END TEXP_TADRESS_BI;
/
SHOW ERRORS;


