DROP TRIGGER ASU.TFREQUENCY_BEFORE_INSERT
/

--
-- TFREQUENCY_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TFREQUENCY (Sequence)
--   TFREQUENCY (Table)
--
CREATE OR REPLACE TRIGGER ASU."TFREQUENCY_BEFORE_INSERT" 
  BEFORE INSERT ON ASU.TFREQUENCY  
  FOR EACH ROW
BEGIN
  IF :NEW.FK_ID IS NULL THEN
    SELECT SEQ_TFREQUENCY.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
  END IF;  
END TFREQUENCY_BEFORE_INSERT;
/
SHOW ERRORS;


