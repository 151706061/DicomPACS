DROP TRIGGER ASU.TFUS_MATERIAL_BEFORE_INS
/

--
-- TFUS_MATERIAL_BEFORE_INS  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TFUS_MATERIAL (Sequence)
--   TFUS_MATERIAL (Table)
--
CREATE OR REPLACE TRIGGER ASU."TFUS_MATERIAL_BEFORE_INS" 
 BEFORE
  INSERT
 ON asu.tfus_material
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
 SELECT SEQ_TFUS_MATERIAL.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


