DROP TRIGGER ASU.THEAL_SOGAZ_CODE_BEFORE_INSERT
/

--
-- THEAL_SOGAZ_CODE_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_THEAL_SOGAZ_CODE (Sequence)
--   THEAL_SOGAZ_CODE (Table)
--
CREATE OR REPLACE TRIGGER ASU.THEAL_SOGAZ_CODE_BEFORE_INSERT
BEFORE INSERT
ON ASU.THEAL_SOGAZ_CODE REFERENCING OLD AS OLD NEW AS NEW
FOR EACH ROW
Begin
  SELECT SEQ_THEAL_SOGAZ_CODE.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


