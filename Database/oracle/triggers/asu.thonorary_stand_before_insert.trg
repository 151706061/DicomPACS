DROP TRIGGER ASU.THONORARY_STAND_BEFORE_INSERT
/

--
-- THONORARY_STAND_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_THONORARY_STANDART (Sequence)
--   THONORARY_STANDART (Table)
--
CREATE OR REPLACE TRIGGER ASU."THONORARY_STAND_BEFORE_INSERT" 
  BEFORE INSERT
  ON ASU.THONORARY_STANDART   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
Begin
  SELECT SEQ_THONORARY_STANDART.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


