DROP TRIGGER ASU.THST0093_BEFORE_INSERT
/

--
-- THST0093_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_THST0093 (Sequence)
--   THST0093 (Table)
--
CREATE OR REPLACE TRIGGER ASU.THST0093_BEFORE_INSERT
  BEFORE INSERT
  ON ASU.THST0093   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
BEGIN
  SELECT ASU.SEQ_THST0093.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
