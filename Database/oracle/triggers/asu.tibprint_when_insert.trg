DROP TRIGGER ASU.TIBPRINT_WHEN_INSERT
/

--
-- TIBPRINT_WHEN_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TIBPRINT (Sequence)
--   TIBPRINT (Table)
--
CREATE OR REPLACE TRIGGER ASU."TIBPRINT_WHEN_INSERT" 
 BEFORE
  INSERT
 ON asu.tibprint
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
 SELECT SEQ_tibprint.NEXTVAL INTO :new.fk_id FROM DUAL;
END;
/
SHOW ERRORS;


