DROP TRIGGER ASU.TINSURDOCS_HEAL_BEFORE_INSERT
/

--
-- TINSURDOCS_HEAL_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TINSURDOCS_HEAL (Sequence)
--   TINSURDOCS_HEAL (Table)
--
CREATE OR REPLACE TRIGGER ASU."TINSURDOCS_HEAL_BEFORE_INSERT" 
 BEFORE
 INSERT
 ON ASU.TINSURDOCS_HEAL  REFERENCING OLD AS OLD NEW AS NEW
 FOR EACH ROW
BEGIN
  SELECT SEQ_TINSURDOCS_HEAL.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


