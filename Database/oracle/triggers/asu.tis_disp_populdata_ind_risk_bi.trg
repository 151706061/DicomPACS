--
-- TIS_DISP_POPULDATA_IND_RISK_BI  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TIS_DISP_POPULDATA_IND_RIS (Sequence)
--   TIS_DISP_POPULDATA_IND_RISK (Table)
--
CREATE OR REPLACE TRIGGER ASU.TIS_DISP_POPULDATA_IND_RISK_BI
  BEFORE INSERT
  ON ASU.TIS_DISP_POPULDATA_IND_RISK   REFERENCING OLD AS OLD NEW AS NEW
  FOR EACH ROW
BEGIN
  IF :NEW.fk_id IS NULL
  THEN
    SELECT asu.seq_TIS_DISP_POPULDATA_IND_RIS.NEXTVAL
      INTO :NEW.fk_id
      FROM DUAL;
  END IF;
END;
/
