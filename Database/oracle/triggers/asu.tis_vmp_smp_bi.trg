DROP TRIGGER ASU.TIS_VMP_SMP_BI
/

--
-- TIS_VMP_SMP_BI  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TIS_VMP_SMP (Sequence)
--   TIS_VMP_SMP (Table)
--
CREATE OR REPLACE TRIGGER ASU.TIS_VMP_SMP_BI
  BEFORE INSERT
  ON ASU.TIS_VMP_SMP   REFERENCING OLD AS OLD NEW AS NEW
  FOR EACH ROW
BEGIN
  IF :NEW.fk_id IS NULL
  THEN
    SELECT asu.seq_TIS_VMP_SMP.NEXTVAL
      INTO :NEW.fk_id
      FROM DUAL;
  END IF;
END;
/
SHOW ERRORS;


