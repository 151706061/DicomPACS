DROP TRIGGER ASU.TISSLRAZDROWS_BEFORE_INSERT
/

--
-- TISSLRAZDROWS_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TISSLRAZDROWS (Sequence)
--   TISSLRAZDROWS (Table)
--
CREATE OR REPLACE TRIGGER ASU."TISSLRAZDROWS_BEFORE_INSERT" 
BEFORE  INSERT  ON ASU.TISSLRAZDROWS REFERENCING
 NEW AS NEW
 OLD AS OLD
FOR EACH ROW
Begin
  SELECT SEQ_TISSLRAZDROWS.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


