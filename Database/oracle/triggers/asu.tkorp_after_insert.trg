DROP TRIGGER ASU.TKORP_AFTER_INSERT
/

--
-- TKORP_AFTER_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   TROOM (Table)
--   TKORP (Table)
--
CREATE OR REPLACE TRIGGER ASU."TKORP_AFTER_INSERT" 
AFTER DELETE
ON ASU.TKORP REFERENCING OLD AS OLD NEW AS NEW
FOR EACH ROW
Begin
  DELETE FROM TROOM WHERE FK_KORPID=:OLD.FK_ID;
End;
/
SHOW ERRORS;


