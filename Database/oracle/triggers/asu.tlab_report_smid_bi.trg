DROP TRIGGER ASU.TLAB_REPORT_SMID_BI
/

--
-- TLAB_REPORT_SMID_BI  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   TLAB_REPORT_SMID (Table)
--   SEQ_LAB_ACCUMULATE (Sequence)
--
CREATE OR REPLACE TRIGGER ASU."TLAB_REPORT_SMID_BI" 
 BEFORE 
 INSERT
 ON ASU.TLAB_REPORT_SMID  REFERENCING OLD AS OLD NEW AS NEW
 FOR EACH ROW
begin
    SELECT SEQ_LAB_ACCUMULATE.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
end;
/
SHOW ERRORS;


