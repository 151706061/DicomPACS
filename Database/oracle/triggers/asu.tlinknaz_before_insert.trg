DROP TRIGGER ASU.TLINKNAZ_BEFORE_INSERT
/

--
-- TLINKNAZ_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TKARTA (Sequence)
--   TLINKNAZ (Table)
--
CREATE OR REPLACE TRIGGER ASU."TLINKNAZ_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON tlinknaz
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT SEQ_TKARTA.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


