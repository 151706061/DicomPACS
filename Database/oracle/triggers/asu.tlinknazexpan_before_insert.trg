DROP TRIGGER ASU.TLINKNAZEXPAN_BEFORE_INSERT
/

--
-- TLINKNAZEXPAN_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TLINKNAZEXPAN (Sequence)
--   TLINKNAZEXPAN (Table)
--
CREATE OR REPLACE TRIGGER ASU."TLINKNAZEXPAN_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON asu.tlinknazexpan
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT asu.SEQ_TLINKNAZEXPAN.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


