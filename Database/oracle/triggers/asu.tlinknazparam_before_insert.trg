DROP TRIGGER ASU.TLINKNAZPARAM_BEFORE_INSERT
/

--
-- TLINKNAZPARAM_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TLINKNAZPARAM (Sequence)
--   TLINKNAZPARAM (Table)
--
CREATE OR REPLACE TRIGGER ASU."TLINKNAZPARAM_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON asu.tlinknazparam
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT asu.SEQ_TLINKNAZPARAM.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


