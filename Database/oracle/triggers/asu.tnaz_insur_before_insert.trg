DROP TRIGGER ASU.TNAZ_INSUR_BEFORE_INSERT
/

--
-- TNAZ_INSUR_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TNAZ_INSUR (Sequence)
--   TNAZ_INSUR (Table)
--
CREATE OR REPLACE TRIGGER ASU."TNAZ_INSUR_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON tnaz_insur
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT SEQ_TNAZ_INSUR.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


