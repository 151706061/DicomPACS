DROP TRIGGER ASU.TNAZMED_PRIEMTIME_BI
/

--
-- TNAZMED_PRIEMTIME_BI  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TKARTA (Sequence)
--   TNAZMED_PRIEMTIME (Table)
--
CREATE OR REPLACE TRIGGER ASU."TNAZMED_PRIEMTIME_BI"
 BEFORE
 INSERT
 ON ASU.TNAZMED_PRIEMTIME  REFERENCING OLD AS OLD NEW AS NEW
 FOR EACH ROW
BEGIN
  IF :NEW.FK_ID IS NULL THEN
    SELECT SEQ_TKARTA.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
  END IF;
END TNAZMED_PRIEMTIME_BI;
/
SHOW ERRORS;


