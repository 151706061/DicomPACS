DROP TRIGGER ASU.TNAZPARAM_BEFORE_INSERT
/

--
-- TNAZPARAM_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TNAZPARAM (Sequence)
--   TNAZPARAM (Table)
--
CREATE OR REPLACE TRIGGER ASU."TNAZPARAM_BEFORE_INSERT" 
BEFORE INSERT
ON ASU.TNAZPARAM REFERENCING OLD AS OLD NEW AS NEW
FOR EACH ROW
begin
  SELECT SEQ_TNAZPARAM.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
end;
/
SHOW ERRORS;


