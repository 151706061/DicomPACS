DROP TRIGGER ASU.TNUMRGISSL_BEFORE_INSERT
/

--
-- TNUMRGISSL_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TNUMRGISSL (Sequence)
--   TNUMRGISSL (Table)
--
CREATE OR REPLACE TRIGGER ASU."TNUMRGISSL_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON asu.tnumrgissl
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT SEQ_TNUMRGISSL.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


