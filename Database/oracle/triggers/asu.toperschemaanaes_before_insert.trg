DROP TRIGGER ASU.TOPERSCHEMAANAES_BEFORE_INSERT
/

--
-- TOPERSCHEMAANAES_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TOPERSCHEMAANAEST (Sequence)
--   TOPERSCHEMAANAEST (Table)
--
CREATE OR REPLACE TRIGGER ASU."TOPERSCHEMAANAES_BEFORE_INSERT" 
  BEFORE INSERT
  ON ASU.TOPERSCHEMAANAEST   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
Begin
  select seq_TOPERSCHEMAANAEST.nextval into :new.fk_id from dual;
End;
/
SHOW ERRORS;


