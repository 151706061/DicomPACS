DROP TRIGGER ASU.TOPERSCHEMAOTDEL_BEFORE_INSERT
/

--
-- TOPERSCHEMAOTDEL_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TOPERSCHEMAOTDEL (Sequence)
--   TOPERSCHEMAOTDEL (Table)
--
CREATE OR REPLACE TRIGGER ASU."TOPERSCHEMAOTDEL_BEFORE_INSERT" 
  BEFORE INSERT
  ON ASU.TOPERSCHEMAOTDEL   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
Begin
  select seq_TOPERSCHEMAOTDEL.nextval into :new.fk_id from dual;
End;
/
SHOW ERRORS;


