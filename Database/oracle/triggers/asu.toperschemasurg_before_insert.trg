DROP TRIGGER ASU.TOPERSCHEMASURG_BEFORE_INSERT
/

--
-- TOPERSCHEMASURG_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TOPERSCHEMASURGEON (Sequence)
--   TOPERSCHEMASURGEON (Table)
--
CREATE OR REPLACE TRIGGER ASU."TOPERSCHEMASURG_BEFORE_INSERT" 
  BEFORE INSERT
  ON ASU.TOPERSCHEMASURGEON   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
Begin
  select seq_TOPERSCHEMASURGEON.nextval into :new.fk_id from dual;
End;
/
SHOW ERRORS;


