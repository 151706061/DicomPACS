DROP TRIGGER ASU.TOTDEL_INSERT
/

--
-- TOTDEL_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TSOTR (Sequence)
--   TOTDEL_BACKUP (Table)
--
CREATE OR REPLACE TRIGGER ASU."TOTDEL_INSERT" 
  BEFORE INSERT ON ASU.TOTDEL_BACKUP   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
Begin
  SELECT Seq_Tsotr.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


