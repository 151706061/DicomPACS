DROP TRIGGER ASU.TOUTSOTRPARAM_BEFORE_INSERT
/

--
-- TOUTSOTRPARAM_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TOUTSOTRPARAM (Sequence)
--   TOUTSOTRPARAM (Table)
--
CREATE OR REPLACE TRIGGER ASU.TOUTSOTRPARAM_BEFORE_INSERT
 BEFORE
 INSERT
 ON ASU.TOUTSOTRPARAM  REFERENCING OLD AS OLD NEW AS NEW
 FOR EACH ROW
Begin
  SELECT ASU.SEQ_TOUTSOTRPARAM.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
 End;
/
SHOW ERRORS;


