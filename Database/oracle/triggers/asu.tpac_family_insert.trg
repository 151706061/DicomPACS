DROP TRIGGER ASU.TPAC_FAMILY_INSERT
/

--
-- TPAC_FAMILY_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TPAC_FAMILY (Sequence)
--   TPAC_FAMILY (Table)
--
CREATE OR REPLACE TRIGGER ASU."TPAC_FAMILY_INSERT" 
 BEFORE
  INSERT
 ON asu.tpac_family
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
Begin
  SELECT SEQ_TPAC_FAMILY.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


