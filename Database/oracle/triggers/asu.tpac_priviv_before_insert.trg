DROP TRIGGER ASU.TPAC_PRIVIV_BEFORE_INSERT
/

--
-- TPAC_PRIVIV_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TPAC_PRIVIV (Sequence)
--   TPAC_PRIVIV (Table)
--
CREATE OR REPLACE TRIGGER ASU."TPAC_PRIVIV_BEFORE_INSERT" 
BEFORE INSERT
ON ASU.TPAC_PRIVIV REFERENCING OLD AS OLD NEW AS NEW
FOR EACH ROW
Begin
  SELECT SEQ_TPAC_PRIVIV.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


