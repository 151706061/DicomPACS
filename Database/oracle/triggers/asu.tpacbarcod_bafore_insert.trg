DROP TRIGGER ASU.TPACBARCOD_BAFORE_INSERT
/

--
-- TPACBARCOD_BAFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TPACBARCOD (Sequence)
--   TPACBARCOD (Table)
--
CREATE OR REPLACE TRIGGER ASU."TPACBARCOD_BAFORE_INSERT" 
 BEFORE
  INSERT
 ON asu.tpacbarcod
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT SEQ_TPACBARCOD.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


