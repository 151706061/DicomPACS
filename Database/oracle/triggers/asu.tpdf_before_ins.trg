DROP TRIGGER ASU.TPDF_BEFORE_INS
/

--
-- TPDF_BEFORE_INS  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TPDF (Sequence)
--   TPDF (Table)
--
CREATE OR REPLACE TRIGGER ASU."TPDF_BEFORE_INS" 
 BEFORE
  INSERT
 ON ASU.TPDF REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
 SELECT SEQ_TPDF.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


