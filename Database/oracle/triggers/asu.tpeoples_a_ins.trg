DROP TRIGGER ASU.TPEOPLES_A_INS
/

--
-- TPEOPLES_A_INS  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   TPEOPLES (Table)
--   TEXP_TPEOPLES (Table)
--
CREATE OR REPLACE TRIGGER ASU."TPEOPLES_A_INS"
 AFTER INSERT
 ON ASU.TPEOPLES  REFERENCING OLD AS OLD NEW AS NEW
 FOR EACH ROW
BEGIN

  INSERT INTO ASU.TEXP_TPEOPLES(FK_PEPLID)
  VALUES(:NEW.FK_ID);

End;
/
SHOW ERRORS;


