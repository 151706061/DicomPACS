DROP TRIGGER ASU.TPRIOR_AFTER_DELETE
/

--
-- TPRIOR_AFTER_DELETE  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   TSPEC (Table)
--   TPRIOR (Table)
--
CREATE OR REPLACE TRIGGER ASU."TPRIOR_AFTER_DELETE" 
  AFTER DELETE ON ASU.TPRIOR   FOR EACH ROW
Begin
  DELETE FROM TSPEC WHERE FK_PRIORID = :OLD.FK_ID;
End;
/
SHOW ERRORS;


