DROP TRIGGER ASU.TPUTPLANDATA_BEFORE_INSERT
/

--
-- TPUTPLANDATA_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TPUTPLANDATA (Sequence)
--   TPUTPLANDATA (Table)
--
CREATE OR REPLACE TRIGGER ASU."TPUTPLANDATA_BEFORE_INSERT" 
BEFORE INSERT
ON ASU.TPUTPLANDATA REFERENCING OLD AS OLD NEW AS NEW
FOR EACH ROW
Begin
  SELECT SEQ_TPutplandata.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


