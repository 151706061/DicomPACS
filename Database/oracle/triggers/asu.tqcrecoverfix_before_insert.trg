DROP TRIGGER ASU.TQCRECOVERFIX_BEFORE_INSERT
/

--
-- TQCRECOVERFIX_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TQCRECOVERFIX (Sequence)
--   TQCRECOVERFIX (Table)
--
CREATE OR REPLACE TRIGGER ASU."TQCRECOVERFIX_BEFORE_INSERT" 
BEFORE  INSERT  ON ASU.TQCRECOVERFIX FOR EACH ROW
Begin
  SELECT SEQ_TQCRECOVERFIX.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


