DROP TRIGGER ASU.TQCSHUHARTSPR_AFTER_DELETE
/

--
-- TQCSHUHARTSPR_AFTER_DELETE  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   TQCSHUHART (Table)
--   TQCSHUHARTFIX (Table)
--   TQCSHUHARTSPR (Table)
--
CREATE OR REPLACE TRIGGER ASU."TQCSHUHARTSPR_AFTER_DELETE" 
AFTER DELETE
ON ASU.TQCSHUHARTSPR REFERENCING OLD AS OLD NEW AS NEW
FOR EACH ROW
Begin
  DELETE FROM TQCSHUHARTFIX WHERE FK_SMID=:OLD.FK_SMID;
  DELETE FROM TQCSHUHART WHERE FK_SMID=:OLD.FK_SMID;
End;
/
SHOW ERRORS;


