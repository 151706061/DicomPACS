DROP TRIGGER ASU.TRECEPTNAZ_BEFORE_INSERT
/

--
-- TRECEPTNAZ_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TRECEPTNAZ (Sequence)
--   TRECEPTNAZ (Table)
--
CREATE OR REPLACE TRIGGER ASU."TRECEPTNAZ_BEFORE_INSERT" 
BEFORE INSERT
ON ASU.TRECEPTNAZ REFERENCING OLD AS OLD NEW AS NEW
FOR EACH ROW
begin
 SELECT SEQ_TRECEPTNAZ.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


