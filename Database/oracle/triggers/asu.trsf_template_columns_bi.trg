DROP TRIGGER ASU.TRSF_TEMPLATE_COLUMNS_BI
/

--
-- TRSF_TEMPLATE_COLUMNS_BI  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TRSF_TEMPLATE_COLUMNS (Sequence)
--   TRSF_TEMPLATE_COLUMNS (Table)
--
CREATE OR REPLACE TRIGGER ASU.TRSF_TEMPLATE_COLUMNS_BI
 BEFORE
 INSERT
 ON ASU.TRSF_TEMPLATE_COLUMNS  REFERENCING OLD AS OLD NEW AS NEW
 FOR EACH ROW
Begin
  SELECT SEQ_TRSF_TEMPLATE_COLUMNS.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


