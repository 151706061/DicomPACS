DROP TRIGGER ASU.TS_COLUMNNAZ_BEFORE_INSERT
/

--
-- TS_COLUMNNAZ_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TS_COLUMNNAZ (Sequence)
--   TS_COLUMNNAZ (Table)
--
CREATE OR REPLACE TRIGGER ASU."TS_COLUMNNAZ_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON asu.ts_columnnaz
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT SEQ_TS_COLUMNNAZ.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


