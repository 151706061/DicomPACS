DROP TRIGGER ASU.TSTATCARDVMP_BEFORE_INSERT
/

--
-- TSTATCARDVMP_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TVMP (Sequence)
--   TSTATCARDVMP (Table)
--
CREATE OR REPLACE TRIGGER ASU.TSTATCARDVMP_BEFORE_INSERT
 BEFORE
 INSERT
 ON ASU.TSTATCARDVMP  REFERENCING OLD AS OLD NEW AS NEW
 FOR EACH ROW
begin
    IF :NEW.FK_ID is NULL then
        SELECT ASU.SEQ_TVMP.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
    end if;
end;
/
SHOW ERRORS;


