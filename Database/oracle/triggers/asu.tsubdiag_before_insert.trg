DROP TRIGGER ASU.TSUBDIAG_BEFORE_INSERT
/

--
-- TSUBDIAG_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TSUBDIAG (Sequence)
--   TSUBDIAG (Table)
--
CREATE OR REPLACE TRIGGER ASU."TSUBDIAG_BEFORE_INSERT" 
  BEFORE INSERT ON ASU.TSUBDIAG   REFERENCING NEW AS NEW OLD AS OLD
  FOR EACH ROW
BEGIN
  SELECT SEQ_TSUBDIAG.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


