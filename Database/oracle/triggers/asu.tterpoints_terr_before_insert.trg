DROP TRIGGER ASU.TTERPOINTS_TERR_BEFORE_INSERT
/

--
-- TTERPOINTS_TERR_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TTHERPOINTS (Sequence)
--   TTHERPOINTS_TERR (Table)
--
CREATE OR REPLACE TRIGGER ASU."TTERPOINTS_TERR_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON ttherpoints_terr
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
begin
  SELECT SEQ_TTHERPOINTS.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
end;
/
SHOW ERRORS;


