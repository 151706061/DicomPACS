DROP TRIGGER ASU.TTUBPARAM_BEFORE_INSERT
/

--
-- TTUBPARAM_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TTUBPARAM (Sequence)
--   TTUBPARAM (Table)
--
CREATE OR REPLACE TRIGGER ASU."TTUBPARAM_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON asu.ttubparam
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
BEGIN
  SELECT SEQ_TTUBPARAM.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
END;
/
SHOW ERRORS;


