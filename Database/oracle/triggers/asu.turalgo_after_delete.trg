DROP TRIGGER ASU.TURALGO_AFTER_DELETE
/

--
-- TURALGO_AFTER_DELETE  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   TURALGO (Table)
--   TURALGOSMID (Table)
--
CREATE OR REPLACE TRIGGER ASU."TURALGO_AFTER_DELETE" 
AFTER  DELETE  ON ASU.TURALGO REFERENCING
 NEW AS NEW
 OLD AS OLD
FOR EACH ROW
Begin
  DELETE FROM TURALGOSMID WHERE FK_ALGOID=:OLD.FK_ID;
End;
/
SHOW ERRORS;


