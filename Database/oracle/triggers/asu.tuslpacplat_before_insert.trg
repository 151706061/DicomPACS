DROP TRIGGER ASU.TUSLPACPLAT_BEFORE_INSERT
/

--
-- TUSLPACPLAT_BEFORE_INSERT  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   DUAL (Synonym)
--   SEQ_TUSLPACPLAT (Sequence)
--   TUSLPACPLAT (Table)
--
CREATE OR REPLACE TRIGGER ASU."TUSLPACPLAT_BEFORE_INSERT" 
 BEFORE
  INSERT
 ON tuslpacplat
REFERENCING NEW AS NEW OLD AS OLD
 FOR EACH ROW
Begin
  SELECT SEQ_tuslPacPlat.NEXTVAL INTO :NEW.FK_ID FROM DUAL;
End;
/
SHOW ERRORS;


