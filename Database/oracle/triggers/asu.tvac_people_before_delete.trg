DROP TRIGGER ASU.TVAC_PEOPLE_BEFORE_DELETE
/

--
-- TVAC_PEOPLE_BEFORE_DELETE  (Trigger) 
--
--  Dependencies: 
--   PKG_VACCIN (Package)
--   TVAC_PEOPLE (Table)
--
CREATE OR REPLACE TRIGGER ASU."TVAC_PEOPLE_BEFORE_DELETE" 
 BEFORE
  DELETE
 ON asu.tvac_people
REFERENCING NEW AS NEW OLD AS OLD
BEGIN
  PKG_VACCIN.INIT_VAC_PLAN_COLLECTION;
END;
/
SHOW ERRORS;


