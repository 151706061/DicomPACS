DROP TRIGGER ASU.VNAZ_A_INS
/

--
-- VNAZ_A_INS  (Trigger) 
--
--  Dependencies: 
--   STANDARD (Package)
--   VNAZ (Table)
--   TEXP_VNAZ (Table)
--
CREATE OR REPLACE TRIGGER ASU."VNAZ_A_INS"
 AFTER INSERT
 ON ASU.VNAZ  REFERENCING OLD AS OLD NEW AS NEW
 FOR EACH ROW
DISABLE
BEGIN
  INSERT INTO ASU.TEXP_VNAZ(FK_NAZID)
  VALUES(:NEW.FK_ID);
End;
/
SHOW ERRORS;


