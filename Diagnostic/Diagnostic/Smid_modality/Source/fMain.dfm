  object odsTree: TOracleDataSet
    SQL.Strings = (
      
        'select rowid, fk_id, fc_name, fk_owner, upper(fc_name) as fc_nam' +
        'e_up,'
      
        '       (select fk_modality from asu.tsmid_modality where fk_smid' +
        ' = t1.fk_id) as fk_modal ,'
      
        '       (select t4.fc_name||'#39'-'#39'||t4.fc_comm from asu.tsmid_modali' +
        'ty t3, asu.tmodality t4 '
      
        '            where t4.fk_id=t3.fk_modality and t3.fk_smid = t1.fk' +
        '_id) as modal_str'
