  object odsPac: TOracleDataSet
    SQL.Strings = (
      'SELECT TPEOPLES_COMPANY.FK_PEOPLEID, '
      '       TPEOPLES_COMPANY.FK_COMPANYID, '
      
        '       (SELECT FC_SHORT FROM TCOMPANY WHERE FK_ID = TPEOPLES_COM' +
        'PANY.FK_COMPANYID) AS COMPANYNAME, '
      
        '       (SELECT FC_NAME FROM TPERIOD_COMPANY WHERE FK_ID = TPEOPL' +
        'ES_COMPANY.FK_PERIODID) AS PERIOD,'
      
        '       (SELECT FK_ID FROM TPERIOD_COMPANY WHERE FK_ID = TPEOPLES' +
        '_COMPANY.FK_PERIODID) AS OWNER, '
      
        '       (SELECT TPEOPLES.FC_FAM||'#39' '#39'||TPEOPLES.FC_IM||'#39' '#39'|| TPEOP' +
        'LES.FC_OTCH FROM TPEOPLES WHERE FK_ID = TPEOPLES_COMPANY.FK_PEOP' +
        'LEID) AS FIO, '
      
        '       DECODE(TO_CHAR((SELECT TPEOPLES.FD_ROJD FROM TPEOPLES WHE' +
        'RE FK_ID = TPEOPLES_COMPANY.FK_PEOPLEID), '#39'dd.mm.yyyy hh24:mi:ss' +
        #39'), '#39'00.00.0000 00:00:00'#39', '
      '              TRUNC(SYSDATE), '
      
        '              (SELECT TPEOPLES.FD_ROJD FROM TPEOPLES WHERE FK_ID' +
        ' = TPEOPLES_COMPANY.FK_PEOPLEID)) FD_ROJD '
      '  FROM TPEOPLES_COMPANY')
