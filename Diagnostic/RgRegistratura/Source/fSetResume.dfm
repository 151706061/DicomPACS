  object odsTree: TOracleDataSet
    SQL.Strings = (
      'SELECT -2 AS FK_ID, '
      '       '#39'...'#39' AS FC_NAME, '
      '       (SELECT DISTINCT FK_OWNER '
      '          FROM ASU.TSHABLON_XRAY_ZAKL'
      '         WHERE FK_OWNER = :PFK_OWNER) FK_OWNER, '
      '       0 AS COUNTCHILD '
      'FROM DUAL'
      ''
      'UNION ALL'
      ''
      'SELECT SHA.FK_ID, '
      '       SHA.FC_NAME, '
      '       SHA.FK_OWNER, '
      
        '       DECODE((SELECT COUNT(FK_ID) FROM ASU.TSHABLON_XRAY_ZAKL W' +
        'HERE FK_OWNER = SHA.FK_ID), 0, 0, 1) AS COUNTCHILD '
      '  FROM ASU.TSHABLON_XRAY_ZAKL SHA'
      ' WHERE SHA.FK_OWNER = :PFK_OWNER'
      '   AND SHA.FK_KABINETID = :PFK_KABINETID'
      'ORDER BY FK_ID'
      '')
    Optimize = False
