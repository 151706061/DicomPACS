  object odsClosedPeriods: TOracleDataSet
    SQL.Strings = (
      'SELECT CD.FK_ID,'
      '       CD.FD_BEGIN,'
      '       CD.FD_END,'
      '       CD.FL_PODPIS,'
      '       decode(CD.FL_PODPIS, 1, '#39#1044#1072#39', '#39#1053#1077#1090#39') fc_podpis,'
      '       CD.FK_MO,'
      '       CD.FD_DATE,'
      '       MO.FC_NAME FC_MO'
      '  FROM MED.TCLOSEDPERIODS CD, MED.TMO MO'
      ' WHERE CD.FK_MO = MO.MOID'
      '   AND CD.FL_DEL <> 1'
      ' ORDER BY CD.FD_BEGIN, CD.FD_END')
    Optimize = False
    QBEDefinition.QBEFieldDefs = {
